[Unit]
Description=Start a stunnel_<%= @_safe_name %> listener

[Service]
Type=simple
ExecStart=/usr/bin/stunnel /etc/stunnel/stunnel_<%= @_safe_name %>.conf
KillMode=process
LimitNOFILE=1048576
LimitNPROC=infinity
